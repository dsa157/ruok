<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" controllerName="com.dsa157.ruok.view.AddRecipient"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Add Recipient">
		<content>
			<HBox>
			<Input id="searchContactsField" />
			<Button id="search" press="onPressSearch" icon="sap-icon://search" />
			</HBox>
		</content>
		<footer>
			<Bar>
				<contentLeft>
					<Button id="back" text="Back" press="onPressBack" icon="sap-icon://nav-back" />
				</contentLeft>
				<contentRight>
					<Button id="save" text="Save" press="onPressSave" icon="sap-icon://save" />
				</contentRight>
			</Bar>
		</footer>
	</Page>
</core:View>

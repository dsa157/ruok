<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="com.dsa157.ruok.view.Schedule" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="My Schedule">
		<content>
            <VBox id="vbox" />
		</content>
        <footer>
          <Bar>
            <contentLeft>
              <Button id="back" text="" press="onPressBack" icon="sap-icon://nav-back" />
            </contentLeft>
            <contentRight>
              <Button id="auto" text="Test" press="onPressAutoAdd" icon="sap-icon://add" />
              <Button id="clear" text="" press="onPressClear" icon="sap-icon://delete" />
              <Button id="refresh" text="" press="onPressRefresh" icon="sap-icon://refresh" />
              <Button id="add" text="Add" press="onPressAdd" icon="sap-icon://add" />
            </contentRight>
          </Bar>
        </footer>
	</Page>
</core:View>
